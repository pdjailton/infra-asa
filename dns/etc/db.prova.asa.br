;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	prova.asa.br. root.prova.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	s1.prova.asa.br.
@	IN	NS	s2.prova.asa.br.

@	IN	A	127.0.0.1
@	IN	AAAA	::1
 

s1 			A	10.25.2.241
s2 			A 	10.25.2.241